{
  "name": "@igorkowalczyk/countdown",
  "description": "Fast, accurate and lightweight countdown & stopwatch written in Typescript.",
  "version": "1.0.2",
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "repository": "IgorKowalczyk/countdown",
  "funding": "https://github.com/sponsors/igorkowalczyk",
  "bugs": "https://github.com/igorkowalczyk/countdown/issues",
  "homepage": "https://igorkowalczyk.github.io/countdown",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "scripts": {
    "build": "rollup -c",
    "format:check": "prettier . --check",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "2.1.0",
    "@igorkowalczyk/prettier-config": "2.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.5",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.3",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.55.0",
    "jest": "29.7.0",
    "prettier": "3.1.0",
    "rollup": "4.6.1",
    "rollup-plugin-uglify": "6.0.4",
    "ts-jest": "29.1.1",
    "typescript": "5.3.2"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "countdown",
    "stopwatch",
    "timer",
    "time",
    "date",
    "javascript",
    "typescript"
  ],
  "packageManager": "pnpm@8.11.0"
