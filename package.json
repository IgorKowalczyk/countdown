{
  "name": "@igorkowalczyk/countdown",
  "description": "Fast, accurate and lightweight countdown & stopwatch written in Typescript.",
  "version": "1.0.1",
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "repository": "IgorKowalczyk/countdown",
  "bugs": "https://github.com/igorkowalczyk/countdown/issues",
  "homepage": "https://igorkowalczyk.github.io/countdown",
  "type": "module",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "scripts": {
    "build": "rollup -c",
    "format:check": "prettier . --check",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "1.5.5",
    "@igorkowalczyk/prettier-config": "1.5.5",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-typescript": "11.1.3",
    "@types/jest": "29.5.4",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.48.0",
    "jest": "29.6.4",
    "prettier": "3.0.2",
    "rollup": "3.28.1",
    "rollup-plugin-polyfill-node": "0.12.0",
    "rollup-plugin-uglify": "6.0.4",
    "ts-jest": "29.1.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "countdown",
    "stopwatch",
    "timer",
    "time",
    "date",
    "javascript",
    "typescript"
  ],
  "packageManager": "pnpm@8.7.0"
}
