{
  "name": "@igorkowalczyk/countdown",
  "description": "Fast, accurate and lightweight countdown & stopwatch written in Typescript.",
  "version": "1.0.2",
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "repository": "IgorKowalczyk/countdown",
  "funding": "https://github.com/sponsors/igorkowalczyk",
  "bugs": "https://github.com/igorkowalczyk/countdown/issues",
  "homepage": "https://igorkowalczyk.github.io/countdown",
  "type": "module",
  "exports": {
    "import": {
      "types": "./dist/mjs/index.d.ts",
      "default": "./dist/mjs/index.js"
    },
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "format:check": "prettier . --check",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "2.2.0",
    "@igorkowalczyk/prettier-config": "2.2.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.12",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "9.3.0",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "rollup": "4.18.0",
    "rollup-plugin-uglify": "6.0.4",
    "ts-jest": "29.1.4",
    "typescript": "5.4.5"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "countdown",
    "stopwatch",
    "timer",
    "time",
    "date",
    "javascript",
    "typescript"
  ]
}
