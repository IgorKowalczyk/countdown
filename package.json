{
  "name": "@igorkowalczyk/countdown",
  "description": "Fast, accurate and lightweight countdown & stopwatch written in Typescript.",
  "version": "1.0.2",
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "repository": "IgorKowalczyk/countdown",
  "funding": "https://github.com/sponsors/igorkowalczyk",
  "bugs": "https://github.com/igorkowalczyk/countdown/issues",
  "homepage": "https://igorkowalczyk.github.io/countdown",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "build": "rollup -c",
    "format:check": "prettier . --check",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "2.2.0",
    "@igorkowalczyk/prettier-config": "2.2.0",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-typescript": "12.1.0",
    "@types/jest": "29.5.13",
    "@types/node": "20.16.11",
    "@typescript-eslint/eslint-plugin": "8.8.1",
    "@typescript-eslint/parser": "8.8.1",
    "eslint": "9.12.0",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "rollup": "4.24.0",
    "rollup-plugin-dts": "6.1.1",
    "rollup-plugin-uglify": "6.0.4",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "countdown",
    "stopwatch",
    "timer",
    "time",
    "date",
    "javascript",
    "typescript"
  ],
  "packageManager": "pnpm@9.12.1"
