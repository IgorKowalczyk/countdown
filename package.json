{
  "name": "@igorkowalczyk/countdown",
  "version": "1.0.0",
  "description": "Fast, accurate and lightweight countdown & stopwatch written in Typescript.",
  "author": "Igor Kowalczyk",
  "bugs": "https://github.com/igorkowalczyk/countdown/issues",
  "repository": "https://github.com/igorkowalczyk/countdown",
  "homepage": "https://igorkowalczyk.github.io/countdown",
  "type": "module",
  "license": "MIT",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "scripts": {
    "build": "rollup -c",
    "format:check": "prettier . --check",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest "
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "1.5.3",
    "@igorkowalczyk/prettier-config": "1.5.3",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.2",
    "@types/jest": "29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "8.45.0",
    "jest": "29.6.1",
    "prettier": "3.0.0",
    "rollup": "3.26.3",
    "rollup-plugin-polyfill-node": "0.12.0",
    "rollup-plugin-uglify": "6.0.4",
    "ts-jest": "29.1.1",
    "tslib": "2.6.0",
    "typescript": "5.1.6"
  },
  "keywords": [
    "countdown",
    "stopwatch",
    "timer",
    "time",
    "date",
    "javascript",
    "typescript"
  ]
}
