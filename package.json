{
  "name": "@igorkowalczyk/countdown",
  "description": "Fast, accurate and lightweight countdown & stopwatch written in Typescript.",
  "version": "1.0.2",
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "repository": "IgorKowalczyk/countdown",
  "funding": "https://github.com/sponsors/igorkowalczyk",
  "bugs": "https://github.com/igorkowalczyk/countdown/issues",
  "homepage": "https://igorkowalczyk.github.io/countdown",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "build": "rollup -c",
    "format:check": "prettier . --check",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "3.0.2",
    "@igorkowalczyk/prettier-config": "3.0.2",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.1",
    "eslint": "9.20.0",
    "jest": "29.7.0",
    "prettier": "3.5.0",
    "rollup": "4.34.0",
    "rollup-plugin-dts": "6.1.1",
    "rollup-plugin-uglify": "6.0.4",
    "ts-jest": "29.2.5",
    "typescript": "5.7.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "countdown",
    "stopwatch",
    "timer",
    "time",
    "date",
    "javascript",
    "typescript"
  ],
  "packageManager": "pnpm@10.3.0"
